package com.hakim.inventory;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import com.hakim.Main;

public class InventoryHandler implements ActionListener {
	
	

	@Override
	public void actionPerformed(ActionEvent e) {

		String yourChoice = e.getActionCommand();
		
		if(Main.scene.inventory == true) {
			
			
			switch(yourChoice) {
			case "item1":
				Inventory.itemUsed();
				break;
			case "item2":
				Inventory.itemUsed();
				break;
			case "item3":
				Inventory.itemUsed();
				break;
			case "item4":
				Inventory.itemUsed();
				break;
			case "item5":
				Inventory.itemUsed();
				break;
			case "item6":
				Inventory.itemUsed();
				break;
			case "item7":
				Inventory.itemUsed();
				break;
			case "item8":
				Inventory.itemUsed();
				break;
			case "item9":
				Inventory.itemUsed();
				break;
			case "item10":
				Inventory.itemUsed();
				break;
				
					
				
				
			}
		}
		
		
		
	}

}
