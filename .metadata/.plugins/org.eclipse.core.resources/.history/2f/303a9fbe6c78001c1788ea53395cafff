package com.hakim.inventory;

public class Equipement {
	String name;
	String rarity;
	String type;
	
	
	Equipement(String name, String rarity, String type) {
		this.name = name;
		this.rarity = rarity;
		this.type = type;
	}
	
	
	public void equipementWhite() {
		// 1 stats
	}
	
	public void equipementGreen() {
		// 2 stats
	}
	
	public void equipementBlue() {
		// 3 stats
	}
	
	public void equipementPurple() {
		// 4 stats
	}
	
	public void equipementOrange() {
		// 5 stats
	}
}
