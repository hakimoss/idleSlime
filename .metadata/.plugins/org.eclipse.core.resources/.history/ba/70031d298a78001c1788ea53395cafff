package com.hakim.item;

import java.io.IOException;
import java.util.Random;

import javax.imageio.ImageIO;

public class Item {
	String name;
	String rarity;
	
	Item(String name, String rarity) {
		this.name = name;
		this.rarity = rarity;
	}
	
	Item(String rarity) {
		this.rarity = rarity;	
	}
}
